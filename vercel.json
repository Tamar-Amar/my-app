{
    "projects": [
      {
        "src": "server/index.js",
        "use": "@vercel/node",
        "routes": [
          { "src": "/api/(.*)", "dest": "/server/index.js" }
        ]
      },
      {
        "src": "client/build/**",
        "use": "@vercel/static-build",
        "config": { "distDir": "build" }
      }
    ]
  }
  